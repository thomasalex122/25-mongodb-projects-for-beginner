<% const convertToASCII = (str) => {
  let converted = str;
  const specialChars = {
    "Á": "a", "á": "a",
    "É": "e", "é": "e",
    "Ñ": "n", "ñ": "n"
  };
  for (const specialChar in specialChars) {
    const regex = new RegExp(specialChar, "g");
    converted = converted.replace(regex, specialChars[specialChar]);
  }
  return converted;
}; %>
